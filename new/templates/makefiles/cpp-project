CXX=g++
CXXFLAGS=-g -Wall -W -Wconversion -Wshadow -Wwrite-strings -Wextra -std=c++17
LDFLAGS=

INCS =
SRCS =
OBJS =

TARGET =

$(TARGET): $(OBJS)
	$(CXX) $^ $(CXXFLAGS) $(LDFLAGS) -o $@

$(OBJS): $(SRCS) $(INCS)
	$(CXX) -c $(CXXFLAGS) $^

.PHONY: clean
clean:
	rm -f *.o

.PHONY: install
install:
	#

.PHONY: uninstall
	#
